@model Images.Report

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAll.cshtml";
}

<head>
    <title>Delete Report</title>
    <link href="~/Content/css-Register/style1.css" rel="stylesheet" />
    <link href="~/Content/css-Register/styleRe.css" rel="stylesheet" />
    <link href="//fonts.googleapis.com/css?family=Noto+Sans:400,400i,700,700i&amp;subset=cyrillic,cyrillic-ext,devanagari,greek,greek-ext,latin-ext,vietnamese" rel="stylesheet">
</head>
<div>
    <div class="w3ls-header">
        <br />
        <br />
        <div class="header-main">
            <h2>Edit Report</h2>
            <hr />
            <div class="header-bottom">
                <div class="header-right w3agile">
                    <div class="header-left-bottom agileinfo">
                        <form action="#" method="post">
                            <div class="form-group">
                                @Html.DisplayNameFor(model => model.sender_email)
                                <div class="icon1">
                                    @Html.DisplayFor(model => model.sender_email)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.DisplayNameFor(model => model.receiver_email)
                                <div class="icon1">

                                    @Html.DisplayFor(model => model.receiver_email)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.DisplayNameFor(model => model.pic_url)
                                <div class="icon1">
                                    @Html.DisplayFor(model => model.pic_url)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.DisplayNameFor(model => model.message)
                                <div class="icon1">
                                    @Html.DisplayFor(model => model.message)
                                </div>
                            </div>

                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()

                                <div class="form-actions no-color">
                                    <input type="submit" value="Delete" class="btn btn-default" /> |
                                    @Html.ActionLink("Back to List", "MyReport")
                                </div>
                            }
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>}


